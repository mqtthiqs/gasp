holtype : type.
i : holtype.
o : holtype.
arr : holtype -> holtype -> holtype.

obj : holtype -> type.
0 : obj(i).
S : obj(arr i i).
+ : obj(arr i (arr i i)).
* : obj(arr i (arr i i)).

lt : obj(arr i (arr i o)).

not : obj(arr o i).
and : obj(arr o (arr o o)).
or : obj(arr o (arr o o)).
imp : obj(arr o (arr o o)).

= : {s : holtype} obj(arr s (arr s o)).
forall : {s : holtype} obj(arr (arr s o) o).
exist : {s : holtype} obj(arr (arr s o) o).

app : {s t : holtype} (obj(arr s t) -> obj s -> obj t).
lam : {s t : holtype} ((obj s -> obj t) -> obj (arr s t)).

true : obj(o) -> type.

all-i : {s : holtype} {F : obj(arr s o)} 
	 ({x : obj s} true(app s o F x)) -> 
	 true(app (arr s o) o (forall s) F).

